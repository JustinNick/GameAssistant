<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.johncheng.gameassistant"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.main.ui.MainActivity">
        </activity>
        <activity android:name=".module.main.ui.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".module.main.ui.RBActivity"/>
        <activity android:name=".module.main.ui.UserRegisterActivity">
        </activity>
        <activity android:name=".module.main.ui.LoginActivity">
        </activity>
        <activity android:name=".module.mygame.ui.MygameActivity">
            <intent-filter>
                <action android:name="tianzhundaoMygameactivity"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>